{
  "author": "Sebastian Prein <hi@sebastianprein.com>",
  "description": "Fast, light and modern web boilerplate.",
  "keywords": [
    "fast",
    "light",
    "modern"
  ],
  "license": "",
  "name": "web-boilerplate",
  "scripts": {
    "build": "node tasks/build",
    "build:fonts": "node tasks/build --task=fonts",
    "build:html": "node tasks/build --task=html",
    "build:images": "node tasks/build --task=images",
    "build:scripts": "node tasks/build --task=scripts",
    "build:sprites": "node tasks/build --task=sprites",
    "build:styles": "node tasks/build --task=styles",
    "clean": "node tasks/build --clean",
    "watch": "node tasks/watch",
    "watch:fonts": "node tasks/watch --task=fonts",
    "watch:html": "node tasks/watch --task=html",
    "watch:images": "node tasks/watch --task=images",
    "watch:scripts": "node tasks/watch --task=scripts",
    "watch:sprites": "node tasks/watch --task=sprites",
    "watch:styles": "node tasks/watch --task=styles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mzdr/web-boilerplate.git"
  },
  "version": "1.0.0",
  "dependencies": {
    "normalize.scss": "^0.1.0",
    "picturefill": "^3.0.1"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "autoprefixer": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.2",
    "fs-extra": "^0.26.4",
    "glob": "^6.0.4",
    "imagemin": "^4.0.0",
    "is-glob": "^2.0.1",
    "node-notifier": "^4.4.0",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.14",
    "replace": "^0.3.0",
    "svg-sprite": "^1.2.19",
    "uglify-js": "^2.6.1"
  }
}
